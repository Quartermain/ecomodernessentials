            window.zbadClickCheckout = function (_currentElement) { //Create Checkout
                var _ztbAdKey = zbGetCookie("_ZB_STATIC_947438_KEY");
                if (_ztbAdKey != undefined && typeof (window.localStorage[_ztbAdKey]) != "undefined") {
